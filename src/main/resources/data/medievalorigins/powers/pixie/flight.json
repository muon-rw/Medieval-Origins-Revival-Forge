{
	"type": "origins:multiple",
	"levitate": {
		"type": "origins:active_self",
		"entity_action": {
			"type": "origins:if_else",
			"condition": {
				"type": "origins:resource",
				"resource": "medievalorigins:pixie/flight_levitate_resource",
				"comparison": "==",
				"compare_to": 0
			},
			"if_action": {
				"type": "origins:and",
				"actions": [
					{
						"type": "origins:execute_command",
						"command": "function medievalorigins:mdvlorigins/pixie_levitate"
					},
					{
						"type": "origins:add_velocity",
						"y": 0,
						"set": true
					}
				]
			},
			"else_action": {
				"type": "origins:execute_command",
				"command": "function medievalorigins:mdvlorigins/pixie_levitate_clear"
			}
		},
		"key": "key.origins.primary_active",
		"cooldown": 0
	},
	"levitate_resource": {
		"type": "origins:resource",
		"min": 0,
		"max": 1,
		"start_value": 0,
		"hud_render": {
			"should_render": false
		}
	},
	"air_slide": {
		"type": "origins:modify_slipperiness",
		"modifier": {
			"operation": "set_total",
			"value": 0.97
		},
		"block_condition": {
			"type": "origins:movement_blocking",
			"inverted": true
		}
	},
	"levitation_alt": {
		"type": "origins:action_over_time",
		"entity_action": {
			"type": "origins:apply_effect",
			"effect": {
				"amplifier": -1,
				"effect": "levitation",
				"duration": 50,
				"show_particles": false,
				"is_ambient": true
			}
		},
		"falling_action": {
			"type": "origins:clear_effect",
			"effect": "levitation"
		},
		"condition": {
			"type": "origins:resource",
			"resource": "medievalorigins:pixie/flight_levitate_resource",
			"compare_to": 1,
			"comparison": "=="
		},
		"interval": 5
	},
	"levitation": {
		"type": "origins:modify_falling",
		"velocity": 0,
		"take_fall_damage": false,
		"condition": {
			"type": "origins:resource",
			"resource": "medievalorigins:pixie/flight_levitate_resource",
			"compare_to": 1,
			"comparison": "=="
		}
	},
	"slow_fall": {
		"type": "origins:modify_falling",
		"velocity": 0.04,
		"take_fall_damage": false,
		"condition": {
			"type": "origins:resource",
			"resource": "medievalorigins:pixie/flight_levitate_resource",
			"compare_to": 0,
			"comparison": "=="
		}
	},
	"no_random_floaties": {
		"type": "origins:action_on_callback",
		"entity_action_added": {
			"type": "origins:execute_command",
			"command": "function medievalorigins:mdvlorigins/pixie_levitate_clear"
		},
		"entity_action_removed": {
			"type": "origins:execute_command",
			"command": "function medievalorigins:mdvlorigins/pixie_levitate_clear"
		},
		"execute_chosen_when_orb": true
	},
	"hover_particles": {
		"type": "origins:action_over_time",
		"condition": {
			"type": "origins:resource",
			"resource": "medievalorigins:pixie/flight_levitate_resource",
			"comparison": "==",
			"compare_to": 1
		},
		"interval": 1,
		"entity_action": {
			"type": "origins:execute_command",
			"command": "particle enchant ~ ~-0.05 ~ 0.1 0.0 0.1 0.05 1 force @a"
		}
	},
	"hover_sound": {
		"type": "origins:action_over_time",
		"condition": {
			"type": "origins:resource",
			"resource": "medievalorigins:pixie/flight_levitate_resource",
			"comparison": "==",
			"compare_to": 1
		},
		"interval": 30,
		"entity_action": {
			"type": "origins:execute_command",
			"command": "playsound medievalorigins:hover player @a ~ ~ ~ 0.5 1.0"
		}
	},
	"dash": {
		"type": "origins:active_self",
		"entity_action": {
			"type": "origins:and",
			"actions": [
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:resource",
						"resource": "medievalorigins:pixie/flight_doubletap",
						"comparison": ">",
						"compare_to": 0
					},
					"if_action": {
						"type": "origins:and",
						"actions": [
							{
								"type": "origins:add_velocity",
								"space": "local_horizontal",
								"z": 2,
								"set": true
							},
							{
								"type": "origins:modify_resource",
								"resource": "medievalorigins:pixie/flight_doubletap",
								"modifier": {
									"operation": "set_total",
									"value": 0
								}
							},
							{
								"type": "origins:execute_command",
								"command": "playsound medievalorigins:dash ambient @a ~ ~ ~ 0.65 0.8"
							},
							{
								"type": "origins:execute_command",
								"command": "playsound medievalorigins:boost ambient @a ~ ~ ~ 0.27 1"
							}
						]
					},
					"else_action": {
						"type": "origins:trigger_cooldown",
						"power": "medievalorigins:pixie/flight_doubletap"
					}
				}
			]
		},
		"key": {
			"key": "key.sprint"
		}
	},
	"effects_and_superjump": {
		"type": "origins:active_self",
		"entity_action": {
			"type": "origins:and",
			"actions": [
				{
					"type": "origins:if_else",
					"condition": {
						"type": "origins:resource",
						"resource": "medievalorigins:pixie/flight_doubletap",
						"comparison": ">",
						"compare_to": 0
					},
					"if_action": {
						"type": "origins:and",
						"actions": [
							{
								"type": "origins:add_velocity",
								"space": "local_horizontal_normalized",
								"y": 1.8,
								"set": true
							},
							{
								"type": "origins:modify_resource",
								"resource": "medievalorigins:pixie/flight_doubletap",
								"modifier": {
									"operation": "set_total",
									"value": 0
								}
							},
							{
								"type": "origins:execute_command",
								"command": "playsound medievalorigins:dash ambient @a ~ ~ ~ 0.65 0.8"
							},
							{
								"type": "origins:execute_command",
								"command": "playsound medievalorigins:boost ambient @a ~ ~ ~ 0.27 1"
							}
						]
					},
					"else_action": {
						"type": "origins:execute_command",
						"command": "function medievalorigins:mdvlorigins/pixie_wing_flap"
					}
				}
			]
		},
		"key": {
			"key": "key.jump"
		}
	},
	"doublejumps": {
		"type": "origins:grounded"
	},
	"higher": {
		"type": "origins:modify_jump",
		"modifier": {
			"operation": "addition",
			"value": 0.3
		},
		"condition": {
			"type": "origins:resource",
			"resource": "medievalorigins:pixie/flight_doubletap",
			"compare_to": 0,
			"comparison": "=="
		}
	},
	"doubletap": {
		"type": "origins:cooldown",
		"cooldown": 10,
		"hud_render": {
			"should_render": true
		}
	}
}