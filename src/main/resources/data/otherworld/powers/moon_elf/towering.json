{
	"type": "origins:multiple",
	"towering_callon": {
		"type": "origins:action_on_callback",
		"entity_action_chosen": {
			"type": "origins:execute_command",
			"command": "function medievalorigins:mdvlorigins/elf_callon"
		},
		"entity_action_lost": {
			"type": "origins:execute_command",
			"command": "function medievalorigins:mdvlorigins/elf_callback"
		},
		"execute_chosen_when_orb": true
	},
	"towering_constant": {
		"type": "origins:action_over_time",
		"entity_action": {
			"type": "origins:execute_command",
			"command": "execute unless entity @s[pehkui.scale_nbt={\"pehkui:scale_data_types\":{\"pehkui:height\":{scale:1.2f}}}] run function medievalorigins:mdvlorigins/elf_callon"
		},
		"interval": 50
	}
}